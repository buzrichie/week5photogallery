{
  "family": "ecs-lab-task",
  "executionRoleArn": "EXECUTION_ROLE_ARN",
  "taskRoleArn": "TASK_ROLE_ARN",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "ecs-lab-container",
      "image": "IMAGE1_NAME",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "LOG_GROUP_NAME",
          "awslogs-region": "AWS_REGION",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "AWS_REGION",
          "value": "AWS_REGION"
        },
        {
          "name": "S3_BUCKET_NAME",
          "value": "ecs-lab-images-AWS_ACCOUNT_ID"
        }
      ],
      "secrets": [
        {
          "name": "DATABASE_URL",
          "valueFrom": "SECRETS_MANAGER_ARN"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}