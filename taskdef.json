{
  "family": "photolab-task",
  "executionRoleArn": "arn:aws:iam::004165097941:role/photolab-ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::004165097941:role/photolab-ecsTaskRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "photolab-container",
      "image": "<IMAGE_URI>",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/photolab",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "AWS_REGION",
          "value": "eu-central-1"
        },
        {
          "name": "S3_BUCKET_NAME",
          "value": "<S3_BUCKET_NAME>"
        }
      ],
      "secrets": [
        {
          "name": "DATABASE_URL",
          "valueFrom": "<SECRETS_MANAGER_ARN>"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}